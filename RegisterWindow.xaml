<Window x:Class="WPFK.RegisterWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Rejestracja" Height="500" Width="500">
    <Grid Margin="20">
        <StackPanel>
            <TextBlock Text="Nazwa użytkownika:" Margin="0,0,0,5"/>
            <TextBox x:Name="UsernameBox" Margin="0,0,0,10" />

            <TextBlock Text="Hasło:" Margin="0,0,0,5"/>
            <PasswordBox x:Name="PasswordBox" Margin="0,0,0,10"/>

            <Button Content="Zarejestruj" Click="Register_Click"/>
        </StackPanel>
    </Grid>
</Window>

